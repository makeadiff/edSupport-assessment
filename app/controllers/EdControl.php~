<?php

class EdControl extends BaseController{

	public function index(){
		$cityId=1;
		$cityName = DB::table('Center')->join('City','Center.city_id','=','City.id')->select('Center.name','Center.id')->where('Center.city_id',$cityId)->where('Center.status','=','1')->get();
		return View::make('content.manage',['cityName'=>$cityName]);
		}

	public function store(){
		
		return View::make('content.report',['cityName'=>$cityName]);
	}

}



